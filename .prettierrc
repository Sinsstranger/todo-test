{
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"editorconfig": true,
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "lf",
	"extends": [
		"prettier"
	],
	"insertPragma": false,
	"jsxBracketSameLine": true,
	"plugins": [
		"prettier"
	],
	"printWidth": 80,
	"proseWrap": "always",
	"requirePragma": false,
	"semi": true,
	"singleAttributePerLine": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "all",
	"useTabs": false
}